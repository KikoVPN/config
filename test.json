{
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  },
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "tag": "remote-dns",
        "address": "tls://9.9.9.9",
        "detour": "proxy"
      },
      {
        "tag": "local-dns",
        "address": "tls://1.1.1.1",
        "detour": "direct"
      },
      {
        "tag": "fakeip-dns",
        "address": "fakeip"
      },
      {
        "tag": "block-dns",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "domain": [
          "vps18.alrab.xyz"
        ],
        "server": "local-dns"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geoip-ir",
          "geosite-cn",
          "geosite-ir"
        ],
        "server": "local-dns"
      },
      {
        "query_type": [
          "A"
        ],
        "rewrite_ttl": 1,
        "server": "fakeip-dns"
      },
      {
        "outbound": "any",
        "server": "remote-dns"
      }
    ],
    "strategy": "ipv4_only",
    "disable_cache": false,
    "final": "remote-dns",
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15"
    }
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 10810,
      "sniff": true
    },
    {
        "type": "tun",
        "tag": "tun-in",
        "inet4_address": "172.19.0.1/30",
        "stack": "gvisor",
        "auto_route": true,
        "strict_route": false,
        "sniff": true,
        "sniff_override_destination": false
    }
  ],
  "outbounds": [
    {
        "type": "hysteria2",
        "tag": "proxy",
        "server": "vps18.alrab.xyz",
        "server_port": 443,
        "up_mbps": 100,
        "down_mbps": 100,
        "obfs": {
            "type": "salamander",
            "password": "1cc6f35af9f02d3b"
        },
        "password": "696d0f7c122784c3e963ab80cac3e596",
        "tls": {
            "enabled": true,
            "server_name": "vps18.alrab.xyz",
            "alpn": ["h3", "h2", "http/1.1"],
            "insecure": false
        }
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "proxy"
      },
      {
        "type": "remote",
        "tag": "geosite-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
        "download_detour": "proxy"
      },
      {
        "type": "remote",
        "tag": "geoip-ir",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geoip-ir.srs",
        "download_detour": "proxy"
      },
      {
        "type": "remote",
        "tag": "geosite-ir",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-ir.srs",
        "download_detour": "proxy"
      }
    ],
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "outbound": "block"
      },
      {
        "package_name": "com.softzium.kikovpn",
        "outbound": "proxy"
      },
      {
        "rule_set": [
          "geosite:category-ads-all"
        ],
        "outbound": "block"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geoip-ir",
          "geosite-cn",
          "geosite-ir"
        ],
        "outbound": "direct"
      }
    ],
    "auto_detect_interface": true,
    "final": "proxy"
  }
}
